<h2>All Stories</h2>
<div class="row">
  {{#each stories}}
  <div class="col s12 m4">
    <div class="card">
      <div class="card-content centre-align">
        <h5>{{title}}</h5>
        <p class="story-text">{{{body}}}</p>
        <br>
        <div class="chip">
          <img src="{{user.image}}" alt="Author"> {{user.firstName}} {{user.lastName}}
        </div>

      </div>
    </div>
  </div>
  {{else}}
  <p>No stories to show.</p>
  {{/each}}
</div>